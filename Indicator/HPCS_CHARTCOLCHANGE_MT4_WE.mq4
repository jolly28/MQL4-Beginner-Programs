//+------------------------------------------------------------------+
//| Custom indicator initialization function                         |
//+------------------------------------------------------------------+
int OnInit()
  {
//--- indicator buffers mapping
   //BACKGROUND COLOR OF CHART
   //ChartSetInteger(0,CHART_COLOR_BACKGROUND,clrBlueViolet);
   
   //RECTANGLE ON CHART
   ObjectSetInteger(0,"obj_rect",OBJPROP_COLOR,clrCoral);
   ObjectCreate(0,"obj_rect",OBJ_RECTANGLE,0,Time[50],Close[50],Time[350],Close[350]);

   
//---
   return(INIT_SUCCEEDED);
  }
//+------------------------------------------------------------------+
//| Custom indicator iteration function                              |
//+------------------------------------------------------------------+
int OnCalculate(const int rates_total,
                const int prev_calculated,
                const datetime &time[],
                const double &open[],
                const double &high[],
                const double &low[],
                const double &close[],
                const long &tick_volume[],
                const long &volume[],
                const int &spread[])
  {
//---
   
//--- return value of prev_calculated for next call
   return(rates_total);
  }
